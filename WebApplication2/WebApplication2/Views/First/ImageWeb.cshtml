@model WebApplication2.Models.ImageWebModel
    @{
    ViewBag.Title = "ImageWeb";
    }


    <h2>ImageWeb</h2>


    @* display- if server is connected and number of images *@  
<div class="alert alert-info" role="alert">
   @Model.IsConnect
</div>
<div class="alert alert-warning" role="alert">
    number of images is @Model.ImagesNum
</div>
    <table id="studentsTable" class="table">
        <tr>
            <th>
                ID
            </th>
            <th>
                First Name
            </th>
            <th>
                Last Name
            </th>
        </tr>
        @* display the details of the students *@
        @foreach (var item in Model.StudentsData)
       {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
        </tr>
        }
    </table>
    
