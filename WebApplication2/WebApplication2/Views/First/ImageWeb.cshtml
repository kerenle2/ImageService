@model WebApplication2.Models.ImageWebModel
    @{
    ViewBag.Title = "ImageWeb";
}


    <h2>ImageWeb</h2>
    <p> @Model.IsConnect</p>
    <p> number of images is @Model.ImagesNum </p>
    <table id="studentsTable" class="table">
        <tr>
            <th>
                ID
            </th>
            <th>
                First Name
            </th>
            <th>
                Last Name
            </th>
        </tr>
        @foreach (var item in Model.StudentsData)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
        </tr>
}
    </table>
    
